{
  "name": "capita_c",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch \"src/\" --exec \"npm run lint:src && npx ts-node src/index.ts\" -e ts",
    "start:build": "node build/index.js",
    "test": "npm run lint:tests && npm run migrate_reset_test && npx dotenv -e env/.test.env jest -- --runInBand",
    "docs": "node scripts/docs.js",
    "ci": "npm run lint:tests && npm run migrate_ci && npx dotenv -e env/.ci.env jest",
    "build": "rimraf build && tsc -p tsconfig.build.json",
    "format": "prettier --write ./src/**/*.ts",
    "prisma_studio_dev": "dotenv -e env/.dev.env prisma studio",
    "prisma_studio_test": "dotenv -e env/.test.env prisma studio",
    "prisma_studio_deploy": "dotenv -e env/.build.env prisma studio",
    "prisma_studio_container": "prisma studio",
    "lint:src": "eslint \"src/**\"",
    "lint:tests": "eslint \"tests/**\"",
    "seed":"npm run migrate_reset_dev && dotenv -e env/.dev.env npx ts-node src/seed.ts",
    "seed:build":"node build/seed.js",
    "migrate_dev": "dotenv -e env/.dev.env prisma migrate dev",
    "migrate_test": "dotenv -e env/.test.env prisma migrate deploy",
    "migrate_ci": "dotenv -e env/.ci.env prisma migrate deploy",
    "migrate_deploy": "dotenv -e env/.build.env prisma migrate deploy",
    "migrate_container": "prisma migrate deploy",
    "migrate_reset_dev": "dotenv -e env/.dev.env prisma migrate reset",
    "migrate_reset_test": "dotenv -e env/.test.env prisma migrate reset -- --force --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "dotenv-cli": "^5.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "http-terminator": "^3.0.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "prisma": "^3.9.2",
    "prisma-docs-generator": "^0.5.0",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
